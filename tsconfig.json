{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": true,
    "checkJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "outDir": "./temp",
    "declaration": true,
    "declarationDir": "./temp",
    "emitDeclarationOnly": true,
    "types": [
      "node"
    ],
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "temp"
  ]
}